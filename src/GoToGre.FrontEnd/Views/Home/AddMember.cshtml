@{
    ViewData["Title"] = "AddMember";
}
@{
    <h1>Add Member</h1>
    <form action="/" method="get" id="user_reg_form">

        <div id="first_name">
            <label for="FirstName">First Name:</label><br>
            <input type="text" name="FirstName" id = "FirstName"required><b><span class="error"></span></b><br>
        </div>

        <div id="last_name">
            <label for="LastName">Last Name:</label><br>
            <input type="text" name="LastName" id="LastName" required> <b><span class="error"></span></b><br><br>
        </div>

        <div id="phone_num">
            <label for="PhoneNumber">Phone Number:</label><br>
            <input type="text" name="PhoneNumber" id="PhoneNumber"><b><span class="error"></span></b><br><br>
        </div>

        <div id="email">
            <label for="Email">Email:</label><br>
            <input type="email" id="Email" name="Email"><b><span class="error"></span></b><br><br>
        </div>
        <div id="address">
            <label for="Address">Address:</label><br>
            <input type="text" name="Address" id="Address" required><b><span class="error"></span></b><br><br>
        </div>
            <input type="submit" value="Submit">
    </form>
}
